{
  "name": "obsidian-jira-sync-pro",
  "version": "1.0.0",
  "description": "Enhanced Obsidian plugin for automatic Jira ticket synchronization",
  "main": "main.js",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build --mode production",
    "build:strict": "tsc -noEmit -skipLibCheck && vite build --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist .turbo node_modules/.cache",
    "setup": "./dev-tools.sh",
    "quick": "npm run build && echo 'âœ… Plugin updated! Reload Obsidian to see changes (Cmd+R).'"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "jira",
    "sync",
    "project-management"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.50.0",
    "jest": "^29.5.0",
    "obsidian": "latest",
    "ts-jest": "^29.1.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {}
}