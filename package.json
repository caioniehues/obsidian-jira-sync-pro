{
  "name": "obsidian-jira-sync-pro",
  "version": "1.0.0",
  "description": "Enhanced Obsidian plugin for automatic Jira ticket synchronization",
  "main": "main.js",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build --mode production",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo node_modules/.cache",
    "setup": "./dev-tools.sh",
    "quick": "npm run build && echo 'âœ… Plugin updated! Reload Obsidian to see changes (Cmd+R).'"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "jira",
    "sync",
    "project-management"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "joi": "^17.11.0",
    "p-queue": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/ui": "^3.2.4",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.50.0",
    "jsdom": "^26.1.0",
    "obsidian": "latest",
    "tslib": "^2.6.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^3.2.4"
  }
}
